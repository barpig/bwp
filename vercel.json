{
    "version": 2,
    "builds": [
      {
        "src": "generate-images.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["images/**", "public/**"]
        }
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "buildCommand": "node generate-images.js",
    "outputDirectory": "public",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1",
        "headers": {
          "Cache-Control": "no-cache"
        }
      }
    ]
  }