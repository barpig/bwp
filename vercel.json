{
    "version": 2,
    "builds": [
      {
        "src": "generate-images.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["images/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ],
    "outputDirectory": "public"
  }